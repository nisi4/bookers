<h2>New book</h2>

<%= form_with model: @book,url: "books",method: :post do |f| %>

<div class="title">
 <h4>Title</h4>
 <%= f.text_field :title %>
</div>

<div class="body">
 <h4>Body</h4>
 <%= f.text_area :body %>
</div>

 <%= f.submit "Create Book" %>

<% end %>